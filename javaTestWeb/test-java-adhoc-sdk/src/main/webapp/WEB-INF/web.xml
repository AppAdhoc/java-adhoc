<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>getflag</servlet-name>
        <servlet-class>net.appadhoc.demo.getFlagServerlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>increamentServlet</servlet-name>
        <servlet-class>net.appadhoc.demo.increamentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>increamentServlet</servlet-name>
        <url-pattern>/increament</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>getflag</servlet-name>
        <url-pattern>/getflag</url-pattern>
    </servlet-mapping>
</web-app>
